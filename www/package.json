    "date-fns": "2.28.0",
    "laravel-mix": "6.0.43",
    "react": "17.0.2",
    "react-bootstrap": "2.2.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.17",
    "react-router-dom": "6.2.2",
